{
  "name": "speedway-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --aot",
    "build:prod": "ng build --prod",
    "start": "npm run env -s && ng serve --aot",
    "start:prod": "npm run env -s && ng serve --prod",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "env": "ngx-scripts env npm_package_version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@angular/animations": "^9.1.10",
    "@angular/cdk": "9.2.4",
    "@angular/common": "^9.1.10",
    "@angular/compiler": "^9.1.10",
    "@angular/core": "^9.1.10",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "^9.1.10",
    "@angular/localize": "^9.1.10",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "^9.1.10",
    "@angular/platform-browser-dynamic": "^9.1.10",
    "@angular/router": "^9.1.10",
    "@angular/service-worker": "^9.1.10",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "firebase": "^7.15.0 ",
    "lodash": "^4.17.15",
    "ng-pick-datetime": "^7.0.0",
    "ngx-clipboard": "^13.0.1",
    "rxjs": "^6.5.5",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "^9.1.10",
    "@angular-devkit/build-angular": "~0.901.7",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.10",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "codelyzer": "^5.2.2",
    "htmlhint": "^0.14.0",
    "jest": "^26.0.1",
    "jest-preset-angular": "^8.2.0",
    "stylelint": "~13.6.0",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.17.2",
    "tslint": "~6.1.2",
    "typescript": "~3.7.2"
  }
}

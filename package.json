{
  "name": "speedway-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --aot",
    "build:prod": "npm run lint && npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot",
    "start:prod": "npm run env -s && ng serve --prod",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "env": "ngx-scripts env npm_package_version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@angular/animations": "^7.2.12",
    "@angular/cdk": "7.3.7",
    "@angular/common": "^7.2.12",
    "@angular/compiler": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "^7.2.12",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "^7.2.12",
    "@angular/platform-browser-dynamic": "^7.2.12",
    "@angular/router": "^7.2.12",
    "@angular/service-worker": "^7.2.12",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "firebase": "^5.9.3",
    "lodash": "^4.17.11",
    "ng-pick-datetime": "^7.0.0",
    "ngx-clipboard": "^12.0.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.12",
    "@angular-devkit/build-angular": "~0.13.8",
    "@ngx-rocket/scripts": "^3.0.2",
    "@types/jasmine": "^3.3.12",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "codelyzer": "^5.0.0",
    "htmlhint": "^0.11.0",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.0.1",
    "stylelint": "~9.10.1",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.4",
    "tslint": "~5.15.0",
    "typescript": "~3.2.4"
  }
}

{
  "name": "speedway-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --aot",
    "build:prod": "ng build --prod --output-path docs --base-href /speedway-app/",
    "start": "npm run env -s && ng serve --aot",
    "start:prod": "npm run env -s && ng serve --prod",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "env": "ngx-scripts env npm_package_version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@angular/animations": "^10.0.3",
    "@angular/cdk": "10.0.1",
    "@angular/common": "^10.0.3",
    "@angular/compiler": "^10.0.3",
    "@angular/core": "^10.0.3",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.0.3",
    "@angular/localize": "^10.0.3",
    "@angular/material": "10.0.1",
    "@angular/platform-browser": "^10.0.3",
    "@angular/platform-browser-dynamic": "^10.0.3",
    "@angular/router": "^10.0.3",
    "@angular/service-worker": "^10.0.3",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "firebase": "^7.16.0 ",
    "lodash": "^4.17.19",
    "ng-pick-datetime": "^7.0.0",
    "ngx-clipboard": "^13.0.1",
    "rxjs": "^6.6.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "^10.0.3",
    "@angular-devkit/build-angular": "~0.901.11",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.11",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.20",
    "codelyzer": "^6.0.0",
    "htmlhint": "^0.14.1",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "stylelint": "~13.6.1",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "tslint": "~6.1.2",
    "typescript": "~3.9.6"
  }
}
